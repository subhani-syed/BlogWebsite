<%- include('partials/header'); -%>

<h1>Home</h1>
<p class="text"><%= Msg%> </p>
      
    <%posts.forEach(function(post){%>
        <h1 class="post-head"><%=post.Title%></h1>
        <p class="post-body"><%=post.Body.substring(0,100)+"..."%>
            <a href="/posts/<%=post.Title%>">Read More</a>
        </p>

    <%})%>

<%- include('partials/footer'); -%>